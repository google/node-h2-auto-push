{
  "name": "h2-auto-push",
  "version": "0.4.1",
  "description": "HTTP/2 auto-push library",
  "main": "build/src/index.js",
  "types": "build/src/index",
  "files": [
    "CONTRIBUTING.md",
    "LICENSE",
    "README.md",
    "build/src"
  ],
  "scripts": {
    "test": "nyc ava",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check && npm run license-check",
    "license-check": "jsgl --local ."
  },
  "engines": {
    "node": ">=9.4.0 || >8.9.4 <9"
  },
  "keywords": [
    "http2",
    "auto-push",
    "server-push"
  ],
  "author": "Google LLC",
  "license": "Apache-2.0",
  "repository": "github:google/node-h2-auto-push",
  "ava": {
    "files": [
      "build/test/*-test.js"
    ]
  },
  "nyc": {
    "exclude": [
      "build/test"
    ]
  },
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "dependencies": {
    "bloomfilter": "^0.0.18",
    "mime-types": "^2.1.22"
  },
  "devDependencies": {
    "@types/get-port": "^4.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^10.0.1",
    "@types/semver": "^5.5.0",
    "ava": "^1.3.1",
    "codecov": "^3.0.1",
    "get-port": "^4.0.0",
    "gts": "^0.9.0",
    "js-green-licenses": "^0.5.0",
    "nyc": "^13.3.0",
    "typescript": "~3.3.0"
  }
}
